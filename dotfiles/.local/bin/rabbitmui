#!/bin/sh
username="$(kubectl get secret -n rabbitmq rabbitmq-default-user -o jsonpath='{.data.username}' | base64 --decode)"
echo "username: $username"
password="$(kubectl get secret -n rabbitmq rabbitmq-default-user -o jsonpath='{.data.password}' | base64 --decode)"
echo "password: $password"

kubectl rabbitmq -n rabbitmq manage rabbitmq
